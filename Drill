SELECT
	trip_id,
	duration
FROM
    trips
WHERE 
    duration > 500
ORDER BY duration


###############################################

SELECT
	*
FROM
    stations
WHERE 
    station_id = 84


################################################

SELECT
    Date,
	MinTemperatureF,
	Events,
	Zip
FROM 
    weather
WHERE 
    Zip=94301 AND 
	Events like 'Rain'
	
  
  
